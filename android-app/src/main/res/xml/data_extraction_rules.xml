<?xml version="1.0" encoding="utf-8"?>
<!--
    Bu kurallar, hangi verilerin Google'ın bulut yedeklemesine dahil edileceğini belirler.
    Güvenlik amacıyla, hassas bilgi içerebilecek alanları (ayarlar, veritabanları)
    varsayılan olarak yedekleme dışında bırakıyoruz.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="sharedpref" />
        <exclude domain="database" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" />
        <exclude domain="database" />
    </device-transfer>
</data-extraction-rules>